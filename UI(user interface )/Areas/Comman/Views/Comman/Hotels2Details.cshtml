@model BOL_business_object_layer_.Hotle_Images_Custom_Model
@{
    ViewBag.Title = "Hotels";
    Layout = "~/Views/Shared/_MainDesign.cshtml";
}

<!--================Home Banner Area =================-->
<section class="banner_area ">
    <div class="banner_inner overlay d-flex align-items-center">
        <div class="container">
            <div class="banner_content">
                <div class="page_link">
                    @Html.ActionLink("Home", "Index", "Comman")

                    @Html.ActionLink("Packages", "Packages", "Comman")
                </div>
                <h2>Trip Packages</h2>
            </div>
        </div>
    </div>
</section>
<!--================End Home Banner Area =================-->
<!--================Blog Area =================-->
<section class="blog_area single-post-area section_gap">
    <div class="container">
        <div class="row">
            <div class="col-lg-8 posts-list">
                <div class="single-post row">
                    <div class="col-lg-12">
                        <div class="feature-img">

                            @*@if (Model.ImagePath != null)
                              {
                             
                             
                                  <img class="img-fluid" src="@Url.Content(Model.ImagePath)" alt="" style="min-width: 100%; max-height: 319px;">
                             
                              }
                              else
                              {
                             
                             
                                  <img src="~/Images/No_Image_Available.jpg" class="img-circle" alt="Avatar" style="min-width: 100%; max-height: 319px;" height="95px" width="85px">
                             
                              }*@
                             

                            <div id="carouselExampleIndicators" class="carousel slide" data-ride="carousel">
                                <ol class="carousel-indicators">
                                    <li data-target="#carouselExampleIndicators" data-slide-to="0" class="active"></li>
                                    <li data-target="#carouselExampleIndicators" data-slide-to="1"></li>
                                    <li data-target="#carouselExampleIndicators" data-slide-to="2"></li>
                                </ol>
                                <div class="carousel-inner" style="max-height: 409px !important;">


                                    @{int i = 0; }
                                    @for (int k = 0; k < Model.Images.Count; k++)
                                    {

                                        i++;
                                        var active = i == 1 ? "active" : "";
                                        <div class="carousel-item @active">
                                            <img class="d-block w-100" src="@Url.Content(Model.Images[k].Images) " alt="First slide" height="420" >
                                        </div>
                                   

                                    }

                                </div>
                                <a class="carousel-control-prev" href="#carouselExampleIndicators" role="button" data-slide="prev">
                                    <span class="carousel-control-prev-icon" aria-hidden="true"></span>
                                    <span class="sr-only">Previous</span>
                                </a>
                                <a class="carousel-control-next" href="#carouselExampleIndicators" role="button" data-slide="next">
                                    <span class="carousel-control-next-icon" aria-hidden="true"></span>
                                    <span class="sr-only">Next</span>
                                </a>
                            </div>

                        </div>
                    </div>
