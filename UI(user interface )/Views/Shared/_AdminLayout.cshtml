
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET Application</title>
    <link href="~/Content/Site.css" rel="stylesheet" type="text/css" />
    <link href="~/Content/bootstrap.min.css" rel="stylesheet" type="text/css" />
    <script src="~/Scripts/modernizr-2.8.3.js"></script>
</head>
<body style="padding:0px;">

    <nav class="navbar navbar-expand-sm bg-dark navbar-dark" style="margin-bottom:20px">
        <!-- Brand/logo -->
        <!-- Links -->
  

      

        


        @Html.ActionLink("NSD", "Index", "Home", new { area = "" }, new { @class = "navbar-brand" })

        <ul class="navbar-nav">

            <li class="nav-item">
                @* AREA LINK *@
                @Html.ActionLink("AdminPanel", "Index", new { Controller = "Admin", area = "Admin" }, new { @class = "nav-link" })
            </li>

            <li class="nav-item">
                @Html.ActionLink("All Reocrd", "AllReocrd", "Admin", new { area = "Admin" }, new { @class = "nav-link " })
            </li>


            <li class="nav-item">
                @Html.ActionLink("Packages", "Index", "Mobile", new { @class = "nav-link" })
                @*<a class="nav-link" href="Mobile/Index">Mobile</a>*@
            </li>

        </ul>


        <ul class="navbar-nav ml-auto">



            @*YE LINE USER JO LOGINED HOGA USKA NAM SHOW KRAY GI*@

            @*<li class="nav-item">
                <a href="#" class="nav-link">@User.Identity.Name </a>

            </li>*@

            <li class="nav-item">
                <a href="#" class="nav-link">@Session["Name"] </a>

            </li>

            <li class="nav-item">

                @Html.ActionLink("Logout", "Logout", "security", new { area = "security" }, new { @class = "nav-link " })

            </li>




        </ul>



        



    </nav>

    <div class="container body-content">
        @RenderBody()
        <hr />
        <footer>
            <p>&copy; @DateTime.Now.Year - My ASP.NET Application</p>
        </footer>
    </div>

    <script src="~/Scripts/jquery-3.4.1.min.js"></script>
    <script src="~/Scripts/bootstrap.min.js"></script>
</body>
</html>